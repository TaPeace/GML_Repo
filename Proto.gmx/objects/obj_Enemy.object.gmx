//Hit by bullet
if place_meeting(x,y,obj_bullet) {
    bullet = instance_place(x,y,obj_bullet);
    splatter = instance_create(bullet.x + lengthdir_x(10,bullet.image_angle),bullet.y + lengthdir_y(10,bullet.image_angle),obj_Bloodsplatter);
    splatter.image_angle = bullet.image_angle;
    if Hp &gt; 0 {
        Kick = 5;
        KickDirection = bullet.direction;
        image_alpha = 0.5;
        x = x + lengthdir_x(Kick,KickDirection);
        y = y + lengthdir_y(Kick,KickDirection);
        Hp -= 1;
    } else {
        instance_destroy();
    }
    with bullet {
        instance_destroy();
    }
} else {
    if image_alpha &lt; 1 {
        image_alpha += 2 / room_speed;
    } else {
        image_alpha = 1;
    }
}


